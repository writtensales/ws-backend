{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "payment": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "trial"
    },
    "lastLoggedIn": {
      "type": "date"
    },
    "created": {
      "type": "date"
    },
    "updated": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "bundles": {
      "type": "hasMany",
      "model": "bundle",
      "foreignKey": ""
    },
    "campaigns": {
      "type": "hasMany",
      "model": "campaign",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
